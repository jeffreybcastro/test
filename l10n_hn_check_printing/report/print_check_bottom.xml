<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>

<template id="print_check_bottom">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
            <div t-att-data-oe-model="o._name" t-att-data-oe-id="o.id" t-foreach="o._check_get_pages()" t-as="page" class="page article ckhn_page ckhn_bottom_page" t-attf-style="top: {{ o.company_id.account_check_printing_margin_top }}in; left: {{ o.company_id.account_check_printing_margin_left }}in;">
                <t t-call="l10n_hn_check_printing.ckhn_check" />
            </div>
        </t>
    </t>
</template>

</data>
</odoo>
